<%= form_with url: search_users_path, method: :get, local: true, html: { class: "search-form-008" } do |f| %>
  <label>
    <%= f.text_field :keyword, placeholder: "検索", class: "form-control" %>
  </label>
  <label>
    <%= f.select :type, options_for_select([['ユーザー', 'user'], ['投稿', 'post']]), {}, { class: 'form-control' } %>
  </label>
  <%= f.submit "検索", class: "search-btn", 'aria-label': "検索" %>
<% end %>


